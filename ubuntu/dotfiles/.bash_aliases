alias defaultscreen='screen -D -RR -S default'

# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias l.='ls -lA'

alias reset_shell='exec $SHELL -l'

# for Windows
alias open='explorer.exe'


# quick load/unload aliases
alias load_tmp='source ~/bin/tmp_aliases.sh'
alias unload_tmp='source ~/bin/tmp_unaliases.sh'


